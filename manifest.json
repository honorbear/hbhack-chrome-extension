{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "http://localhost:3000/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/bee16.png",
      "32": "images/bee32.png",
      "48": "images/bee48.png",
      "128": "images/bee128.png"
    }
  },
  "icons": {
    "16": "images/bee16.png",
    "32": "images/bee32.png",
    "48": "images/bee48.png",
    "128": "images/bee128.png"
  },
  "web_accessible_resources": [
    "images/*.png"
  ]
}
